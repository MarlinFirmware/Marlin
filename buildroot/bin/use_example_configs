#!/usr/bin/env bash

restore_configs

cp Marlin/src/config/examples/"$@"/Configuration* Marlin/

[ -f "Marlin/src/config/examples/$@/_Bootscreen.h" ] && cp "Marlin/src/config/examples/$@/_Bootscreen.h" Marlin/
[ -f "Marlin/src/config/examples/$@/_Statusscreen.h" ] && cp "Marlin/src/config/examples/$@/_Statusscreen.h" Marlin/
