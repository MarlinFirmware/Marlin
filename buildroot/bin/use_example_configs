#!/usr/bin/env bash

restore_configs

EXAMPLES="https://raw.githubusercontent.com/MarlinFirmware/Configurations/master/config/examples"

cd Marlin

curl -LJO "$EXAMPLES/$@/Configuration.h"
curl -LJO "$EXAMPLES/$@/Configuration_adv.h"
curl -LJO "$EXAMPLES/$@/_Bootscreen.h"
curl -LJO "$EXAMPLES/$@/_Statusscreen.h"

cd -
